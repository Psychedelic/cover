{
  "version": 1,
  "dfx": "0.8.4",
  "canisters": {
    "cover": {
      "build": "node build.js --name cover",
      "candid": "services/cover/cover.did",
      "wasm": "target/wasm32-unknown-unknown/release/cover.wasm",
      "type": "custom"
    },
    "blackhole": {
      "build": "node build.js --name blackhole",
      "candid": "services/blackhole/blackhole.did",
      "wasm": "target/wasm32-unknown-unknown/release/blackhole.wasm",
      "type": "custom"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
