{
  "version": 1,
  "dfx": "0.8.4",
  "canisters": {
    "cover": {
      "build": [
        "cargo build --target wasm32-unknown-unknown --package cover --release --bin cover",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/cover.wasm -o target/wasm32-unknown-unknown/release/opt.wasm"
      ],
      "candid": "cover.did",
      "wasm": "target/wasm32-unknown-unknown/release/opt.wasm",
      "type": "custom"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
