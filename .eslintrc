{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
  ],
  "rules": {
    "import/no-unresolved": [2, { "ignore": ["^@dfx\/(.*)"] }],
    "import/no-extraneous-dependencies": "off",
  }
}
