type BuildParams = record {
  git_ref: text;
  git_shat: text;
};

type ValidationRequest = record {
  validation_id: opt nat32;
  caller_id: principal;
  canister_id: principal;
  fetched: bool;
};

type ValidationResult = record {
  git_ref: text;
  git_shat: text;
};

service: {
    whoami: () -> (principal);
    my_validations: () -> (vec ValidationRequest);
    // Builder API
    fresh_validations: () -> (vec principal);
}
